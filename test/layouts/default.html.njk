<head>
<meta charset="utf-8" />
<!-- Use the .htaccess and remove these lines to avoid edge case issues. More info: h5bp.com/i/378 -->
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
<title>{{ title}}</title>
{% import "ak_core_macros.njk" as ak_core with context %}
{{ ak_core.stylesheets() }}
{{ ak_core.headerJavaScript() }}

{% import "ak_base_macros.njk" as ak_base with context %}
{{ ak_base.headerMeta() }}
{# {% include "ak_headermeta.html.njk" %} #}
{# <partial file-name="ak_headermeta.html.njk"/> #}
{# <ak-header-metatags></ak-header-metatags> #}
{{ ak_base.linkRelTags() }}
{# <ak-header-linkreltags></ak-header-linkreltags> #}
{{ ak_base.canonicalURL() }}
{# <ak-header-canonical-url></ak-header-canonical-url> #}
<open-graph-promote-images root="article"></open-graph-promote-images>
</head>
<body>

<h1>{{ title }}</h1>

<article>
    {{ content }}

    {#  template="video-players-from-urls.html.njk" #}

        <video-players-from-videourls/>

        <video-thumbnail-from-videourls></video-thumbnail-from-videourls>

</article>


</body>
</html>
