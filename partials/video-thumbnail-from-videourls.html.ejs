<%
  if (typeof videoUrls !== "undefined" && videoUrls && videoUrls[0] && videoUrls[0].url) {
  	if (videoUrls[0].url.match(/vimeo.com/)) {
    	%><embed-thumbnail width="200" align="right" url="<%= videoUrls[0].url %>"/><%
    } else if (videoUrls[0].url.match(/youtube.com/)) {
    	%><embed-thumbnail width="200" align="right" href="<%= videoUrls[0].url %>"/><%
    } else {
    	%><embed-thumbnail width="200" align="right" href="<%= videoUrls[0].url %>"/><%
    }
  } %>
