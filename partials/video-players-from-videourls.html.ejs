
<%
  if (typeof videoUrls !== "undefined") {
    videoUrls.forEach(function(videoData) {
    	if (videoData.url.match(/vimeo.com/)) { %>
			<framed-vimeo-player <%
				if (videoData.hideDescription) { %>data-hide-description='<%= videoData.hideDescription %>' <% }
			%> url='<%= videoData.url %>'></framed-vimeo-player><%
		} if (videoData.url.match(/youtube.com/)) { %>
			<framed-youtube-player autohide="1" modestbranding="0" rel="0" showinfo="1" iv_load_policy="3" <%
				if (videoData.hideDescription) { %>data-hide-description='<%= videoData.hideDescription %>' <% }
			%> url='<%= videoData.url %>'></framed-youtube-player><%
		}
    })
  } %>